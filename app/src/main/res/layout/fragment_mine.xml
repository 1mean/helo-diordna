    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@color/white"
            app:cardElevation="0dp"
            app:cardCornerRadius="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="15dp">

        <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_download"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/clayout_download"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_download"
                        app:layout_constraintRight_toRightOf="@id/clayout_download"
                        android:src="@mipmap/img_self_down" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/str_mine_download"
                        android:textSize="12sp"
                        android:textColor="@color/color_mine_txt_download"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_download"
                        app:layout_constraintRight_toRightOf="@id/clayout_download"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_download"
                        android:layout_marginTop="5dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_history"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/clayout_history"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_history"
                        app:layout_constraintRight_toRightOf="@id/clayout_history"
                        android:src="@mipmap/img_self_history" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/str_mine_history"
                        android:textSize="12sp"
                        android:textColor="@color/color_mine_txt_download"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_history"
                        app:layout_constraintRight_toRightOf="@id/clayout_history"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_history"
                        android:layout_marginTop="5dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_collect"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/clayout_collect"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_collect"
                        app:layout_constraintRight_toRightOf="@id/clayout_collect"
                        android:src="@mipmap/img_self_collect1" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/str_mine_collect"
                        android:textSize="12sp"
                        android:textColor="@color/color_mine_txt_download"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_collect"
                        app:layout_constraintRight_toRightOf="@id/clayout_collect"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_collect"
                        android:layout_marginTop="5dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_later"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/clayout_later"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_later"
                        app:layout_constraintRight_toRightOf="@id/clayout_later"
                        android:src="@mipmap/img_self_later" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/str_mine_later"
                        android:textSize="12sp"
                        android:textColor="@color/color_mine_txt_download"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_later"
                        app:layout_constraintRight_toRightOf="@id/clayout_later"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_later"
                        android:layout_marginTop="5dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.cardview.widget.CardView>
    <!--<androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="161dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:cardCornerRadius="10dp"
            android:layout_marginTop="20dp"
            app:cardElevation="0.3dp"
            android:background="@color/white">-->

    <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:layout_marginTop="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

        <androidx.cardview.widget.CardView
                android:id="@+id/cv_error"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:cardCornerRadius="15dp"
                android:layout_marginEnd="10dp"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/white">

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_log_text1"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="15dp" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="日志文件"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="13sp"
                    android:includeFontPadding="false"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="38dp" />

            <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="35dp">

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_error_log"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="错误日志、崩溃文件"
                        android:textColor="@color/color_text_setting_info"
                        android:textSize="10sp"
                        android:layout_gravity="center_vertical"
                        android:includeFontPadding="false"
                        tools:ignore="SmallSp" />

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="7dp"
                        android:src="@mipmap/img_self_arrow_right" />
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
                android:id="@+id/cv_file"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/white">

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/img_log_text1"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="15dp" />

            <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目缓存"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="13sp"
                    android:includeFontPadding="false"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="38dp" />

            <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:orientation="horizontal"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="36dp">

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_file"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="错误日志、崩溃文件"
                        android:textColor="@color/color_text_setting_info"
                        android:textSize="10sp"
                        android:layout_gravity="center_vertical"
                        android:includeFontPadding="false"
                        tools:ignore="SmallSp" />

                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="7dp"
                        android:src="@mipmap/img_self_arrow_right" />
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.cardview.widget.CardView>
    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:cardCornerRadius="15dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="15dp"
            app:cardElevation="0dp"
            app:cardBackgroundColor="@color/white">

        <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="更多服务"
                    android:textColor="@color/color_video_item_title"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="18dp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_setting"
                    android:layout_marginTop="10dp"
                    style="@style/style_self_setting_item">

                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/img_left_setting"
                        app:layout_constraintTop_toTopOf="@id/clayout_setting"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_setting"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_setting"
                        style="@style/style_self_setting_left_image"
                        android:src="@mipmap/img_left_setting" />

                <androidx.appcompat.widget.AppCompatTextView
                        style="@style/style_self_setting_name"
                        app:layout_constraintTop_toTopOf="@id/clayout_setting"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_setting"
                        app:layout_constraintLeft_toRightOf="@id/img_left_setting"
                        android:text="@string/str_setting" />

                <androidx.appcompat.widget.AppCompatTextView
                        style="@style/style_self_setting_desc"
                        app:layout_constraintTop_toTopOf="@id/clayout_setting"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_setting"
                        app:layout_constraintRight_toRightOf="@id/clayout_setting"
                        android:text="@string/str_setting_info" />

                <androidx.appcompat.widget.AppCompatImageView
                        style="@style/style_self_setting_arrow"
                        app:layout_constraintTop_toTopOf="@id/clayout_setting"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_setting"
                        app:layout_constraintRight_toRightOf="@id/clayout_setting" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_other_setting"
                    style="@style/style_self_setting_item">

                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/img_left_outlock"
                        app:layout_constraintTop_toTopOf="@id/clayout_other_setting"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_other_setting"
