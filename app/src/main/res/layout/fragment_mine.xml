<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_bg_love2"
        android:orientation="vertical">

    <View
            android:layout_width="match_parent"
            android:layout_height="32.66dp"
            android:background="@color/color_white_lucency" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_mine_top"
            android:layout_width="match_parent"
            android:layout_height="44dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_update_bg"
                android:layout_width="42dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="@id/layout_mine_top"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toRightOf="@id/layout_mine_top">

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_update_bg"
                    app:layout_constraintBottom_toBottomOf="@id/item_update_bg"
                    app:layout_constraintRight_toRightOf="@id/item_update_bg"
                    app:layout_constraintLeft_toLeftOf="@id/item_update_bg"
                    android:src="@mipmap/img_mine_dark" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_character"
                android:layout_width="42dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="@id/layout_mine_top"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toLeftOf="@id/item_update_bg">

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_character"
                    app:layout_constraintBottom_toBottomOf="@id/item_character"
                    app:layout_constraintRight_toRightOf="@id/item_character"
                    app:layout_constraintLeft_toLeftOf="@id/item_character"
                    android:src="@mipmap/img_clothes" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_mine_scan"
                android:layout_width="42dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="@id/layout_mine_top"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toLeftOf="@id/item_character">

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_mine_scan"
                    app:layout_constraintBottom_toBottomOf="@id/item_mine_scan"
                    app:layout_constraintRight_toRightOf="@id/item_mine_scan"
                    app:layout_constraintLeft_toLeftOf="@id/item_mine_scan"
                    android:src="@mipmap/img_mine_scan" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/view_header"
            android:layout_width="match_parent"
            android:layout_height="90dp">

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_mine_header"
                android:layout_width="64dp"
                android:layout_height="64dp"
                android:scaleType="centerCrop"
                app:layout_constraintTop_toTopOf="@id/view_header"
                app:layout_constraintBottom_toBottomOf="@id/view_header"
                app:layout_constraintLeft_toLeftOf="@id/view_header"
                android:layout_marginStart="17dp" />

        <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_mine_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/img_mine_header"
                app:layout_constraintLeft_toRightOf="@id/img_mine_header"
                android:layout_marginStart="18dp"
                android:layout_marginTop="5dp"
                android:textSize="16sp"
                android:textColor="@color/color_mine_txt_name"
                android:text="和叶小可怜" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_mine_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toRightOf="@id/txt_mine_name"
                android:layout_marginStart="3dp"
                app:layout_constraintTop_toTopOf="@id/txt_mine_name"
                android:layout_marginTop="6dp"
                android:src="@mipmap/img_mine_male" />

        <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_mine_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toRightOf="@id/img_mine_sex"
                android:layout_marginStart="5dp"
                app:layout_constraintTop_toTopOf="@id/txt_mine_name"
                android:layout_marginTop="1dp"
                android:src="@mipmap/img_level2" />

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clayout_self"
                android:layout_width="60dp"
                android:layout_height="40dp"
                app:layout_constraintBottom_toBottomOf="@id/view_header"
                app:layout_constraintRight_toRightOf="@id/view_header"
                android:layout_marginBottom="5dp"
                android:layout_marginEnd="15dp">

            <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/clayout_self"
                    app:layout_constraintBottom_toBottomOf="@id/clayout_self"
                    app:layout_constraintLeft_toLeftOf="@id/clayout_self"
                    android:text="个人中心"
                    android:includeFontPadding="false"
                    android:textSize="12sp"
                    android:textColor="@color/color_text_setting_info" />

            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/clayout_self"
                    app:layout_constraintBottom_toBottomOf="@id/clayout_self"
                    app:layout_constraintRight_toRightOf="@id/clayout_self"
                    android:src="@mipmap/img_self_arrow_right" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_coins"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="硬币：20"
                android:textSize="12sp"
                android:textColor="@color/color_txt_tab"
                android:gravity="center_vertical"
                android:includeFontPadding="false"
                app:layout_constraintBottom_toBottomOf="@id/img_mine_header"
                app:layout_constraintLeft_toLeftOf="@id/txt_mine_name"
                android:layout_marginBottom="5dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clayout_mine_zone"
                style="@style/style_mine_top_clayout">

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txt_mine_zone"
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_zone"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_zone"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_zone"
                    android:text="15"
                    style="@style/style_mine_top_text" />

            <androidx.appcompat.widget.AppCompatTextView
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_zone"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_zone"
                    app:layout_constraintTop_toBottomOf="@id/txt_mine_zone"
                    style="@style/style_mine_top_text_name"
                    android:text="@string/str_zone" />

            <View
                    style="@style/style_mine_top_drivider"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_zone"
                    app:layout_constraintBottom_toBottomOf="@id/clayout_mine_zone"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_zone" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clayout_mine_follow"
                style="@style/style_mine_top_clayout">

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txt_mine_follow"
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_follow"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_follow"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_follow"
                    style="@style/style_mine_top_text" />

            <androidx.appcompat.widget.AppCompatTextView
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_follow"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_follow"
                    app:layout_constraintTop_toBottomOf="@id/txt_mine_follow"
                    style="@style/style_mine_top_text_name"
                    android:text="@string/str_follow" />

            <View
                    style="@style/style_mine_top_drivider"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_follow"
                    app:layout_constraintBottom_toBottomOf="@id/clayout_mine_follow"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_follow" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clayout_mine_fans"
                style="@style/style_mine_top_clayout">

            <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txt_mine_fans"
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_fans"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_fans"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_fans"
                    android:text="6"
                    style="@style/style_mine_top_text" />

            <androidx.appcompat.widget.AppCompatTextView
                    app:layout_constraintLeft_toLeftOf="@id/clayout_mine_fans"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_fans"
                    app:layout_constraintTop_toBottomOf="@id/txt_mine_fans"
                    style="@style/style_mine_top_text_name"
                    android:text="@string/str_fans" />

            <View
                    style="@style/style_mine_top_drivider"
                    app:layout_constraintTop_toTopOf="@id/clayout_mine_fans"
                    app:layout_constraintBottom_toBottomOf="@id/clayout_mine_fans"
                    app:layout_constraintRight_toRightOf="@id/clayout_mine_fans" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.appcompat.widget.LinearLayoutCompat>

    <!--<View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/color_bg_search"
            android:layout_marginTop="10dp" />-->
