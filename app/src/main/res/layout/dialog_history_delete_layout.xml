<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/shape_dialog_delete">

    <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/txt_popu_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginStart="23dp"
            android:textColor="@color/color_txt_tab_select"
            android:text="@string/str_delte_history" />

    <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/txt_history_dialog_yes"
            android:layout_width="64dp"
            android:layout_height="54dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginEnd="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="4dp"
            android:background="@null"
            android:textColor="#FF5E9A"
            android:textSize="14sp"
            android:text="@string/str_sure" />

    <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/txt_history_dialog_no"
            android:layout_width="64dp"
            android:layout_height="54dp"
            app:layout_constraintRight_toLeftOf="@id/txt_history_dialog_yes"
            android:layout_marginEnd="12dp"
            app:layout_constraintBottom_toBottomOf="@id/txt_history_dialog_yes"
            android:background="@null"
            android:textColor="#FF5E9A"
            android:textSize="14sp"
            android:text="@string/str_cannel" />
</androidx.constraintlayout.widget.ConstraintLayout>