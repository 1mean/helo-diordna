<?xml version="1.0" encoding="utf-8"?>
<com.kproduce.roundcorners.RoundFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_self_clayout_bg"
        app:rTopLeftRadius="20dp"
        app:rTopRightRadius="20dp">

    <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/color_self_clayout_bg">

        <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


            <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_cache_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"
                    android:overScrollMode="never" />

        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</com.kproduce.roundcorners.RoundFrameLayout>