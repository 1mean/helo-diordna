<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_bg_home"
        android:orientation="vertical">

    <View
            android:layout_width="match_parent"
            android:layout_height="32.66dp"
            android:background="@color/color_white_lucency" />

    <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout_mine_top"
            android:layout_width="match_parent"
            android:layout_height="45dp">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_mode_dark"
                android:layout_width="36dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="15dp"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toRightOf="@id/layout_mine_top">

            <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_update_dark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_mode_dark"
                    app:layout_constraintBottom_toBottomOf="@id/item_mode_dark"
                    app:layout_constraintRight_toRightOf="@id/item_mode_dark"
                    app:layout_constraintLeft_toLeftOf="@id/item_mode_dark"
                    android:src="@mipmap/img_dark_new_black" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_character"
                android:layout_width="36dp"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toLeftOf="@id/item_mode_dark">

            <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_update_background"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_character"
                    app:layout_constraintBottom_toBottomOf="@id/item_character"
                    app:layout_constraintRight_toRightOf="@id/item_character"
                    app:layout_constraintLeft_toLeftOf="@id/item_character"
                    android:src="@mipmap/img_clothes_new_black" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/item_mine_search"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="@id/layout_mine_top"
                app:layout_constraintRight_toLeftOf="@id/item_character">

            <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/img_update_search"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/item_mine_search"
                    app:layout_constraintBottom_toBottomOf="@id/item_mine_search"
                    app:layout_constraintRight_toRightOf="@id/item_mine_search"
                    app:layout_constraintLeft_toLeftOf="@id/item_mine_search"
                    android:src="@mipmap/img_search_new_black" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never">

        <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_top_info"
                    android:layout_width="match_parent"
                    android:layout_height="110dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_top_header"
                        app:layout_constraintTop_toTopOf="@id/clayout_top_info"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_top_info"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginStart="16dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/img_mine_header"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="centerCrop"
                            app:layout_constraintTop_toTopOf="@id/clayout_top_header"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_top_header"
                            app:layout_constraintRight_toRightOf="@id/clayout_top_header"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_top_header"
                            app:civ_border_width="2dp"
                            android:src="@mipmap/img_icon_seadog"
                            app:civ_border_color="@color/white" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_mine_login"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        app:layout_constraintLeft_toRightOf="@id/clayout_top_header"
                        app:layout_constraintTop_toTopOf="@id/clayout_top_header"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_top_header"
                        android:includeFontPadding="false"
                        android:textSize="16sp"
                        android:visibility="gone"
                        android:textColor="@color/white"
                        android:text="点击登录" />

                <androidx.constraintlayout.widget.Group
                        android:id="@+id/group_mine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:constraint_referenced_ids="txt_mine_name,img_mine_sex,clayout_self,txt_coins" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_mine_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/clayout_top_info"
                        app:layout_constraintLeft_toRightOf="@id/clayout_top_header"
                        android:layout_marginTop="26dp"
                        android:layout_marginStart="10dp"
                        android:textSize="19sp"
                        android:textColor="@color/color_txt_mine_name" />

                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/img_mine_sex"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/txt_mine_name"
                        app:layout_constraintLeft_toLeftOf="@id/txt_mine_name"
                        android:layout_marginTop="10dp" />

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/img_mine_level"
                        android:layout_width="27dp"
                        android:layout_height="14dp"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:text="LV.0"
                        android:textSize="9sp"
                        android:textColor="#818891"
                        android:background="@drawable/shape_bg_level"
                        app:layout_constraintLeft_toRightOf="@id/txt_mine_name"
                        app:layout_constraintTop_toTopOf="@id/txt_mine_name"
                        app:layout_constraintBottom_toBottomOf="@id/txt_mine_name"
                        android:layout_marginStart="10dp"
                        tools:ignore="SmallSp" />

                <!--android:text="ID：24870949"-->
                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_coins"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/color_txt_tab"
                        android:gravity="center_vertical"
                        android:includeFontPadding="false"
                        app:layout_constraintTop_toTopOf="@id/img_mine_sex"
                        app:layout_constraintLeft_toRightOf="@id/img_mine_sex" />

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_self"
                        android:layout_width="wrap_content"
                        android:layout_height="28dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_top_info"
                        app:layout_constraintRight_toRightOf="@id/clayout_top_info"
                        android:layout_marginTop="60dp"
                        android:layout_marginEnd="15dp">

                    <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/txt_self_zone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/clayout_self"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_self"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_self"
                            android:layout_marginStart="12dp"
                            android:text="@string/str_mine_zone"
                            android:includeFontPadding="false"
                            android:textSize="11sp"
                            android:textColor="#98a4ad" />

                    <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/img_zone_to"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/clayout_self"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_self"
                            app:layout_constraintLeft_toRightOf="@id/txt_self_zone"
                            android:src="@mipmap/img_setting_top_arror2" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_mine_title"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginTop="11dp"
                    android:background="@drawable/shape_mine_title">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_mine_attention"
                        android:layout_width="164.5dp"
                        android:layout_height="match_parent"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_title">

                    <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/txt_mine_attention"
                            style="@style/style_mine_setting_fans"
                            app:layout_constraintTop_toTopOf="@id/clayout_mine_attention"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_mine_attention"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_mine_attention"
                            app:layout_constraintRight_toRightOf="@id/clayout_mine_attention"
                            android:text="@string/str_attention" />

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/txt_mine_attention"
                            app:layout_constraintBottom_toBottomOf="@id/txt_mine_attention"
                            app:layout_constraintRight_toLeftOf="@id/txt_mine_attention"
                            android:layout_marginEnd="10dp"
                            android:src="@mipmap/img_mine_attention" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                        android:layout_width="1dp"
                        android:layout_height="24dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_mine_title"
                        app:layout_constraintBottom_toBottomOf="@id/clayout_mine_title"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_title"
                        app:layout_constraintRight_toRightOf="@id/clayout_mine_title"
                        android:background="#eedfc5" />

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_mine_fans"
                        android:layout_width="164.5dp"
                        android:layout_height="match_parent"
                        app:layout_constraintRight_toRightOf="@id/clayout_mine_title">

                    <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/txt_mine_fans"
                            style="@style/style_mine_setting_fans"
                            app:layout_constraintTop_toTopOf="@id/clayout_mine_fans"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_mine_fans"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_mine_fans"
                            app:layout_constraintRight_toRightOf="@id/clayout_mine_fans"
                            android:text="@string/str_fans" />

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/txt_mine_fans"
                            app:layout_constraintBottom_toBottomOf="@id/txt_mine_fans"
                            app:layout_constraintRight_toLeftOf="@id/txt_mine_fans"
                            android:layout_marginEnd="10dp"
                            android:src="@mipmap/img_mine_fans" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_kugou_style_setting"
                    android:layout_width="match_parent"
                    android:layout_height="146dp"
                    android:layout_marginTop="20dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style1"
                        android:layout_width="103dp"
                        android:layout_height="match_parent"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style_setting"
                        android:layout_marginStart="15dp"
                        android:background="@drawable/shape_mine_kugou_style1">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="110dp"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style1"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style1"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style1"
                            android:src="@mipmap/img_kakaxi" />

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style1"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style1"
                            android:layout_marginStart="13dp"
                            android:layout_marginBottom="8dp"
                            android:text="@string/str_history" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style2"
                        android:layout_width="71dp"
                        android:layout_height="71dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_kugou_style_setting"
                        app:layout_constraintLeft_toRightOf="@id/clayout_kugou_style1"
                        android:layout_marginStart="6dp"
                        android:background="@drawable/shape_mine_kugou_style2">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style2"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style2"
                            android:src="@mipmap/img_mingren" />

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style2"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style2"
                            android:layout_marginStart="13dp"
                            android:layout_marginBottom="8dp"
                            android:text="缓存" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style3"
                        android:layout_width="146dp"
                        android:layout_height="71dp"
                        app:layout_constraintLeft_toRightOf="@id/clayout_kugou_style2"
                        android:layout_marginStart="4dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_kugou_style_setting"
                        android:background="@drawable/shape_mine_kugou_style3">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style3"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style3"
                            android:src="@mipmap/img_zzxy" />

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style3"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style3"
                            android:layout_marginStart="13dp"
                            android:layout_marginBottom="8dp"
                            android:text="我喜欢" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style4"
                        android:layout_width="71dp"
                        android:layout_height="71dp"
                        android:background="@drawable/shape_mine_kugou_style4"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style2"
                        app:layout_constraintTop_toBottomOf="@id/clayout_kugou_style2"
                        android:layout_marginTop="4dp">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style4"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style4"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="5dp"
                            android:src="@mipmap/img_hua1" />

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style4"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style4"
                            android:layout_marginStart="13dp"
                            android:layout_marginBottom="8dp"
                            android:text="收藏" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style5"
                        android:layout_width="71dp"
                        android:layout_height="71dp"
                        app:layout_constraintLeft_toRightOf="@id/clayout_kugou_style4"
                        app:layout_constraintTop_toTopOf="@id/clayout_kugou_style4"
                        android:layout_marginStart="4dp"
                        android:background="@drawable/shape_mine_kugou_style5">

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style5"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style5"
                            android:layout_marginTop="10dp"
                            android:maxEms="2"
                            android:lines="2"
                            android:paddingStart="4dp"
                            android:lineSpacingExtra="5dp"
                            android:text="稍后再看" />

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style5"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style5"
                            android:layout_marginBottom="5dp"
                            android:layout_marginLeft="5dp"
                            android:src="@mipmap/img_hua1" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/clayout_kugou_style6"
                        android:layout_width="71dp"
                        android:layout_height="71dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_kugou_style5"
                        app:layout_constraintLeft_toRightOf="@id/clayout_kugou_style5"
                        android:layout_marginStart="4dp"
                        android:background="@drawable/shape_mine_kugou_style6">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style6"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style6"
                            app:layout_constraintTop_toTopOf="@id/clayout_kugou_style6"
                            android:layout_marginTop="5dp"
                            android:src="@mipmap/img_hua2" />

                    <androidx.appcompat.widget.AppCompatTextView
                            style="@style/style_txt_mine_collect"
                            app:layout_constraintLeft_toLeftOf="@id/clayout_kugou_style6"
                            app:layout_constraintRight_toRightOf="@id/clayout_kugou_style6"
                            app:layout_constraintBottom_toBottomOf="@id/clayout_kugou_style6"
                            android:layout_marginBottom="8dp"
                            android:text="积分" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_mine_function"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="0dp"
                    app:cardBackgroundColor="@color/color_bg_cardview_self"
                    android:layout_marginTop="15dp">

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_mine_more_function"
                        android:layout_width="wrap_content"
                        android:layout_height="45dp"
                        android:text="更多功能"
                        android:textColor="@color/color_txt_morefunc_self"
                        android:textSize="15sp"
                        android:gravity="center_vertical"
                        android:drawableStart="@mipmap/img_mine_function"
                        android:drawablePadding="7dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_mine_function"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_function"
                        android:includeFontPadding="false"
                        android:layout_marginStart="15dp" />

                <androidx.appcompat.widget.LinearLayoutCompat
                        android:id="@+id/clayout_setting_function"
                        android:layout_width="match_parent"
                        android:layout_height="63dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        app:layout_constraintTop_toBottomOf="@id/txt_mine_more_function"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_function"
                        android:orientation="horizontal">

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_mine_authentication"
                            android:layout_marginStart="15dp"
                            style="@style/style_user_top_info">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_authentication" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="实名认证" />
                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_mine_younger"
                            style="@style/style_user_top_info"
                            android:layout_marginStart="7dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_yonger" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="青少年模式" />

                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_mine_safe"
                            style="@style/style_user_top_info"
                            android:layout_marginStart="7dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_safe" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="账号安全" />

                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_mine_chat"
                            style="@style/style_user_top_info"
                            android:layout_marginStart="7dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_phone" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="联系我们" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                        android:id="@+id/clayout_setting_function2"
                        android:layout_width="match_parent"
                        android:layout_height="63dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="10dp"
                        app:layout_constraintTop_toBottomOf="@id/clayout_setting_function"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_function"
                        android:orientation="horizontal">

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/clayout_setting_timing"
                            style="@style/style_user_top_info"
                            android:layout_marginStart="15dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_time" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="定时关闭" />
                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/clayout_mine_setting"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="0dp"
                    app:cardBackgroundColor="@color/color_bg_cardview_self"
                    android:layout_marginTop="15dp">

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_mine_more_setting"
                        android:layout_width="wrap_content"
                        android:layout_height="45dp"
                        android:text="其他服务"
                        android:textColor="@color/color_txt_morefunc_self"
                        android:textSize="15sp"
                        android:gravity="center_vertical"
                        android:drawableStart="@mipmap/img_mine_function2"
                        android:drawablePadding="7dp"
                        app:layout_constraintTop_toTopOf="@id/clayout_mine_setting"
                        app:layout_constraintLeft_toLeftOf="@id/clayout_mine_setting"
                        android:includeFontPadding="false"
                        android:layout_marginStart="15dp" />

                <androidx.appcompat.widget.LinearLayoutCompat
                        android:id="@+id/llayout_mine_setting"
                        android:layout_width="match_parent"
                        android:layout_height="63dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        app:layout_constraintTop_toBottomOf="@id/txt_mine_more_setting"
                        app:layout_constraintLeft_toLeftOf="@id/txt_mine_more_setting"
                        android:orientation="horizontal">

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_setting"
                            android:layout_marginStart="15dp"
                            style="@style/style_user_top_info">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_mine_setting" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="设置" />
                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_setting_about"
                            style="@style/style_user_top_info"
                            android:layout_marginStart="7dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_function_about" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="关于项目" />

                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/llayout_setting_loginout"
                            android:layout_width="150dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@drawable/shape_bg_item_function"
                            android:layout_marginStart="7dp">

                        <androidx.appcompat.widget.AppCompatImageButton
                                style="@style/style_function_top"
                                android:src="@mipmap/img_mine_loginout" />

                        <androidx.appcompat.widget.AppCompatTextView
                                style="@style/style_function_bottom"
                                android:text="退出登录" />

                    </androidx.appcompat.widget.LinearLayoutCompat>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                    android:layout_width="match_parent"
                    android:layout_height="200dp" />

        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</androidx.appcompat.widget.LinearLayoutCompat>
